page {
  background-color: #dce7fc;
}
.contain .head {
  background-color: #ffffff;
  color: #3F92F7;
  font-weight: bolder;
  display: flex;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin: 10rpx;
  border: solid #3F92F7 3rpx;
}
.contain .head view {
  font-size: 30rpx;
  text-align: center;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
}
.contain .title {
  color: #3F92F7;
  font-size: 40rpx;
  margin-left: 30rpx;
  border-bottom: #3F92F7;
  border-bottom-style: solid;
  width: 200rpx;
  font-weight: bolder;
}
.contain .content {
  background-color: #ffffff;
  border: solid #3F92F7 6rpx;
  border-radius: 30rpx;
  margin: 20rpx auto;
  width: 85%;
  padding: 20rpx;
  height: 170rpx;
}
.contain .content .roomTitle {
  color: #919191;
  font-size: 40rpx;
  font-weight: bold;
}
.contain .content .number {
  color: #9fa3a3;
  margin-top: 30rpx;
}
.contain .detail-page {
  width: 100%;
  height: 100%;
  min-height: 300px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  flex-wrap: nowrap;
}
.contain .detail-page .submit {
  position: absolute;
  right: 40rpx;
  top: 40rpx;
  color: #f2bd10;
  font-size: 30rpx;
}
.contain .detail-page input {
  width: 80%;
  height: 50rpx;
  border: solid #3F92F7 4rpx;
  border-radius: 40rpx;
  margin: 20rpx;
  padding: 30rpx;
  background-color: #ebebeb;
  color: #095ad4;
}
